{
	"name": "liminal-tauri-plugins",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=20.19.6 <21",
		"pnpm": "10.28.2"
	},
	"packageManager": "pnpm@10.28.2",
	"scripts": {
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint .",
		"build": "pnpm --filter \"./plugins/**\" --parallel build",
		"test": "pnpm test:rust && pnpm test:js",
		"test:rust": "cargo test --workspace",
		"test:js": "vitest run"
	},
	"devDependencies": {
		"@eslint/js": "^9.0.0",
		"eslint": "^9.0.0",
		"prettier": "^3.0.0",
		"typescript": "^5.3.0",
		"vitest": "^4.0.18"
	}
}
